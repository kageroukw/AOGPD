@model Dispatcher

@{
    ViewData["Title"] = "Main";
}

<div class="text-center">
    <h1 class="display-4">Choose Your Side</h1>
    <hr />
    <form asp-action="newdispatch" enctype="multipart/form-data" method="post" class="mt-2">
        <fieldset><legend>Dispatch Login</legend></fieldset>
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Username</label>
            <div class="col-sm-10">
                <input asp-for="UserName" class="form-control" type="text" required />
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Password</label>
            <div class="col-sm-10">
                <input asp-for="Password" class="form-control" type="password" required />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-2">
                <a asp-action="Create"><input class="btn btn-primary" type="submit" value="Register New" /></a>
            </div>
        </div>
    </form>
</div>